<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wili.android.gdanskit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CompaniesActivity"
            android:label="@string/companies"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EventsActivity"
            android:label="@string/events"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CoworkingActivity"
            android:label="@string/coworking"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AcademiesActivity"
            android:label="@string/academies"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>